.dialog_box {
  position: fixed;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
}
.dialog_box .slot_box {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  width: 100%;
}
.dialog_box .mask {
  background: rgba(0, 0, 0, 0.2);
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
}

